<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script src="https://reeteshghimire.com.np/wp-content/uploads/2021/05/html5-qrcode.min_.js"></script>
    <script src="https://cdn.tailwindcss.com"></script>
    <title>Apple Student Community</title>
    <script defer="defer" src="/static/js/main.3a5a5cea.js"></script>
    <link href="/static/css/main.28f2623b.css" rel="stylesheet" />
    <title>Document</title>
  </head>
  <body>
    <div class="justify-center" id="reader"></div>
    <div id="root"></div>
  </body>
  <script>
    const onScanSuccess = async (qrCodeMessage) => {
      try {
        await fetch("/adminpanel/qrscanner", {
          method: "POST",
          headers: {
            "Content-Type": "application/json",
          },
          body: JSON.stringify({
            id: qrCodeMessage,
          }),
        })
        .then((response) => response.json())
        .then((data) => {
            alert(JSON.stringify(data));
        });
      } catch (e) {
        alert(`error: ${e}`);
      }
    };
    var html5QrcodeScanner = new Html5QrcodeScanner("reader", {
      fps: 10,
      qrbox: 250,
    });
    html5QrcodeScanner.render(onScanSuccess, (errorMessage) => {});
  </script>
</html>
